while m + n + p > 0
{ -m - n - p + 1 ≤ 0 }
  if m > 0
{ -m + 1 ≤ 0 /\ -m - n - p + 1 ≤ 0 }
    m = m - 1
  else
{ m ≤ 0 /\ -m - n - p + 1 ≤ 0 }
    if n > 10
{ -n + 11 ≤ 0 /\ m ≤ 0 /\ -m - n - p + 1 ≤ 0 }
      n = n + -10
    else
{ n - 10 ≤ 0 /\ m ≤ 0 /\ -m - n - p + 1 ≤ 0 }
      p = p - 1
{ m + n + p ≤ 0 }
