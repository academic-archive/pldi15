{ T }
assert y >= 0;
while x - y > 0
  (
{ -x + y + 1 ≤ 0 /\ -y ≤ 0 }
    x = x - 1;
{ -x + y ≤ 0 /\ -y ≤ 0 }
    x = x - y;
{ -x ≤ 0 /\ -y ≤ 0 }
    z = y;
    while z > 0
{ -z + 1 ≤ 0 /\ -y ≤ 0 /\ -x ≤ 0 /\ -y + z ≤ 0 }
      z = z - 1
{ -z ≤ 0 /\ z ≤ 0 /\ -y + z ≤ 0 /\ -x ≤ 0 /\ -y ≤ 0 }
  )
{ x - y ≤ 0 /\ -y ≤ 0 }
