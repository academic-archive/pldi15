{ T }
assert x > 0;
{ -x + 1 ≤ 0 }
assert y > 0;
while 1 > 0
{ -y + 1 ≤ 0 /\ -x + 1 ≤ 0 }
  if x < y
{ x - y + 1 ≤ 0 /\ -y + 1 ≤ 0 /\ -x + 1 ≤ 0 }
    y = y - x
  else
{ -x + y ≤ 0 /\ -y + 1 ≤ 0 /\ -x + 1 ≤ 0 }
    if y < x
{ -x + y + 1 ≤ 0 /\ -x + y ≤ 0 /\ -y + 1 ≤ 0 /\ -x + 1 ≤ 0 }
      x = x - y
    else
{ x - y ≤ 0 /\ -x + y ≤ 0 /\ -y + 1 ≤ 0 /\ -x + 1 ≤ 0 }
      break
{ 1 ≤ 0 /\ -y + 1 ≤ 0 /\ -x + 1 ≤ 0 }
